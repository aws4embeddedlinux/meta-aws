From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: Yocto Builder <builder@yoctoproject.org>
Date: Mon, 1 Jan 2024 00:00:00 +0000
Subject: [PATCH] Downgrade Cargo.lock to version 3 and update crate versions

Rust 1.75.0 in Yocto Scarthgap does not support Cargo.lock version 4.
Also update cc and find-msvc-tools to match vendored versions.

Upstream-Status: Inappropriate [Yocto-specific compatibility fix]

Signed-off-by: Yocto Builder <builder@yoctoproject.org>
---
 rust/Cargo.lock | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/rust/Cargo.lock b/rust/Cargo.lock
index 1111111..2222222 100644
--- a/rust/Cargo.lock
+++ b/rust/Cargo.lock
@@ -1,10 +1,10 @@
 # This file is automatically @generated by Cargo.
 # It is not intended for manual editing.
-version = 4
+version = 3
 
 [[package]]
 name = "cc"
-version = "1.2.44"
+version = "1.2.47"
 source = "registry+https://github.com/rust-lang/crates.io-index"
-checksum = "37521ac7aabe3d13122dc382493e20c9416f299d2ccd5b3a5340a2570cdeb0f3"
+checksum = "cd405d82c84ff7f35739f175f67d8b9fb7687a0e84ccdc78bd3568839827cf07"
 dependencies = [
@@ -14,7 +14,7 @@ dependencies = [
 
 [[package]]
 name = "find-msvc-tools"
-version = "0.1.4"
+version = "0.1.5"
 source = "registry+https://github.com/rust-lang/crates.io-index"
-checksum = "52051878f80a721bb68ebfbc930e07b65ba72f2da88968ea5c06fd6ca3d3a127"
+checksum = "3a3076410a55c90011c298b04d0cfa770b00fa04e1e3c97d3f6c9de105a03844"
 
-- 
2.34.1
